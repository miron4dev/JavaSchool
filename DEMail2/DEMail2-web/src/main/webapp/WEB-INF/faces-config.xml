<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

<validator>
    <validator-id>phoneValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.PhoneValidation</validator-class>
</validator>

<validator>
    <validator-id>passValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.PasswordValidation</validator-class>
</validator>

<validator>
    <validator-id>bDateValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.BDateValidation</validator-class>
</validator>

<validator>
    <validator-id>fNameValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.FNameValidation</validator-class>
</validator>

<validator>
    <validator-id>lNameValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.LNameValidation</validator-class>
</validator>

<validator>
    <validator-id>rtPassValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.RTPasswordValidation</validator-class>
</validator>

<validator>
    <validator-id>uNameValidation</validator-id>
    <validator-class>com.tsystems.demail.Validation.UNameValidation</validator-class>
</validator>
    
</faces-config>
